{
    "name": "oncology-support/sparkline-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle that ease the use of Davaxi/Sparkline to display sparkline graphs in your app",
    "keywords": ["symfony", "sparkline", "charts", "chart", "graph", "graphs", "davaxi"],
    "homepage": "https://github.com/ehymel/SparklineBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ernest Hymel"
        }
    ],
    "support": {
        "issues": "https://github.com/ehymel/SparklineBundle/issues",
        "source": "https://github.com/ehymel/SparklineBundle/releases"
    },
    "require": {
        "php": ">=8.2",
        "davaxi/sparkline": "dev-patch-1",
        "symfony/http-kernel": "^6.0 || ^7.0",
        "symfony/dependency-injection": "^6.0 || ^7.0",
        "symfony/config": "^6.0 || ^7.0",
        "symfony/yaml": "^6.0 || ^7.0",
        "twig/twig": "^2.10 || ^3.0",
        "symfony/flex": "^2.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "symfony/framework-bundle": "^4.4 || ^5.0 || ^6.0 || ^7.0",
        "nyholm/symfony-bundle-test": "^1.6.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sgiehl/Sparkline.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "OncologySupport\\Sparkline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OncologySupport\\Sparkline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.0-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
